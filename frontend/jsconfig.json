{
  "include": [
    "vite.config.*",
    "env.d.ts",
    "shims.d.ts",
    "src/**/*",
    "themeConfig.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "src/@iconify/*"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["src/*"],
      "@layouts/*": ["src/@layouts/*"],
      "@layouts": ["src/@layouts"],
      "@core/*": ["src/@core/*"],
      "@core": ["src/@core"],
      "@axios": ["src/plugins/axios"]
    },
    "types": [
      "vite/client"
    ]
  }
}
